<div>
    <h1>Real Time Products</h1>

    <h3>Lista de productos:</h3>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Description</th>
                <th>Price</th>
                <th>Thumbnail</th>
                <th>Code</th>
                <th>Stock</th>
            </tr>
        </thead>
        <tbody id="productos-body">

        </tbody>
    </table>

    <h2>Agregar Producto</h2>

    <form id="producto-form">
        <label for="title">Título</label>
        <input type="text" id="title" name="title" requerid>

        <label for="description">Descripción</label>
        <input type="text" id="description" name="description" requerid>

        <label for="price">Precio</label>
        <input type="number" id="price" name="price" requerid>

        <label for="code">Código</label>
        <input type="text" id="code" name="code" requerid>

        <label for="stock">Stock</label>
        <input type="number" id="stock" name="stock" requerid>

        <button type="submit">Agregar Producto</button>

    </form>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="js/index.js"></script>